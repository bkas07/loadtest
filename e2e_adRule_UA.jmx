              <stringProp name="parameters"></stringProp>
              <stringProp name="script">def count = vars.get(&quot;mrouter_url_matchNr&quot;) as int

if (count &gt; 0) {
    def lastValue = vars.get(&quot;mrouter_url_&quot; + count)
    def prevValue = vars.get(&quot;last_mrouter_url&quot;)  // get previous value if any

    // store current value
    vars.put(&quot;last_mrouter_url&quot;, lastValue)

    // also store previous value separately
    vars.put(&quot;previous_mrouter_url&quot;, prevValue ?: &quot;&quot;) // handle null on first run
}
</stringProp>
              <stringProp name="scriptLanguage">groovy</stringProp>
