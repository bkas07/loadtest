              <stringProp name="script">def count = vars.get(&quot;mrouter_url_matchNr&quot;) as int
if (count &gt; 0) {
    def lastValue = vars.get(&quot;mrouter_url_&quot; + count)
    vars.put(&quot;last_mrouter_url&quot;, lastValue)
}
</stringProp>
