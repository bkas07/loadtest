        <JSR223Listener guiclass="TestBeanGUI" testclass="JSR223Listener" testname="JSR223 Listener">
          <stringProp name="cacheKey">true</stringProp>
          <stringProp name="filename"></stringProp>
          <stringProp name="parameters"></stringProp>
          <stringProp name="script">// JSR223 Listener (Groovy)

// Only log HTTP samplers (optional filter)
if (sampleEvent.getResult().getSampleLabel().toLowerCase().contains(&quot;http&quot;)) {
    def samplerName = sampleEvent.getResult().getSampleLabel()
    def responseCode = sampleEvent.getResult().getResponseCode()
    def responseMessage = sampleEvent.getResult().getResponseMessage()
    def responseTime = sampleEvent.getResult().getTime()
    def threadName = sampleEvent.getResult().getThreadName()

    def logLine = &quot;Sampler: ${samplerName} | Code: ${responseCode} | Msg: ${responseMessage} | Time: ${responseTime} ms | Thread: ${threadName}\n&quot;

    // Write to clean log file
    new File(&quot;/tests/live_responses.log&quot;) &lt;&lt; logLine

    // Also send to JMeter console log
    log.info(logLine)
}
</stringProp>
          <stringProp name="scriptLanguage">groovy</stringProp>
